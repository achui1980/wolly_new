	<queryString><![CDATA[select c.customer_short_name,
    sum(fn_currency_change(if(oStatus=0,runing,0),o.currency_id,order_rate)) AS Oruning,
    sum(fn_currency_change(if(ooStatus=2,shipped,0),o.currency_id,order_rate)) AS Oshipped,
    sum(totalprofit) as totalProfit,avg(averageProfit) as averageProfit,
    (sum(fn_currency_change(if(oStatus=0,runing,0),o.currency_id,order_rate)) + sum(fn_currency_change(if(ooStatus=2 ,shipped,0),o.currency_id,order_rate)))AS TOTAL
    from v_cust_rank o,cot_customer c
where o.cust_id = c.id
and o.order_lc_date BETWEEN  $P{STARTTIME} AND $P{ENDTIME}
group by o.cust_id
order by total desc]]></queryString>

